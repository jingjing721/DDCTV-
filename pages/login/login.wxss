.wraper{
    width: 100%;
    height: 100%;
    background-color: #fff;
    overflow: hidden;
    text-align: center;
    font-size: 32rpx;
}
.title{
	margin-top: 400rpx;
	margin-bottom: 80rpx;
}
.btn{
	position: relative;
	width: 300rpx;
	height: 100rpx;
	line-height: 100rpx;
	background-color: #333;
	border-radius: 50rpx;
	color: #fff;
	font-weight: bold;
	margin:0 auto;
}
.btn button{
	position: absolute;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	opacity: 0;
}