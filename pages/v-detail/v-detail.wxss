.wraper{
	width: 100%;
	height: 100%;
	overflow:hidden;
	position: relative;
}
.video{
	width: 100%;
	height: 420rpx;
	position: absolute;
	left: 0;
	top:0;
	right:0;
}
.video image{
	width: 100%;
	height: 100%;
}
.video video{
	width: 100%;
	height: 100%;
}
.video .icon{
	width: 84rpx;
	height: 84rpx;
	position: absolute;
	left: 50%;
	top: 50%;
	margin:-42rpx 0 0 -42rpx;
	z-index: 2;
	background-image: url('data:image/png;base64,');
	background-size: 100%;
	background-position: center center;
	background-repeat: no-repeat;
}
.video2{
	height: 420rpx;
}
.video2 image{
	width: 100%;
	height: 100%;
}
.content{
	width: auto;
	position: absolute;
	left: 0;
	top: 420rpx;
	right: 0;
	bottom:0;
	z-index: 1;
}
.content.top{
	top: 0;
}
.content .title{
	font-size: 36rpx;
	font-weight: bold;
	color: #1A1A1A;
	padding: 40rpx 40rpx 0 40rpx;
}
.content .tags{
	overflow:hidden;
	margin:20rpx 0 26rpx 0;
}
.content text{
	display: block;
	font-size: 28rpx;
	line-height: 28rpx;
	padding: 6rpx 10rpx;
	color: #fff;
	border-radius: 6rpx;
	float:left;
	background-color: #F6B112;
}
.content .main{
	color: #474747;
	margin-top: 40rpx;
	overflow:hidden;
	font-size: 28rpx;
	line-height: 1.5;
	padding:0 40rpx;
}
.praise{
	position: fixed;
	left: 50%;
	bottom: 0;
	margin-left: -100rpx;
	width: 200rpx;
	height: 100rpx;
	padding-top: 100rpx;
	border-radius: 50%;
	font-size: 24rpx;
	color: #A5A4A4;
	background-repeat: no-repeat;
	background-size: 100%;
	background-position: center center;
	background-image: url('data:image/png;base64,');
	text-align: center;
	-webkit-transition: all 0.2s linear;
	-moz-transition: all 0.2s linear;
	-ms-transition: all 0.2s linear;
	-o-transition: all 0.2s linear;
	transition: all 0.2s linear;
	z-index: 10000;
}
.praise button{
	position: absolute;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	opacity: 0;
}
.praise.on{
	color: #FF5D31;
	background-image: url('data:image/png;base64,');
}


.list{
	margin-top: 40rpx;
	line-height: 1.5;
}
.list .item .word{
	padding: 0 40rpx;
}
.list image{
	margin:20rpx 0;
	width: 100%;
}










