.wraper{
	width: 100%;
	height: 100%;
	overflow:hidden;
	position: relative;
}
.video{
	width: 100%;
	height: 420rpx;
	position: absolute;
	left: 0;
	top:0;
	right:0;
}
.video image{
	width: 100%;
	height: 100%;
}
.video video{
	width: 100%;
	height: 100%;
}
.video .icon{
	width: 84rpx;
	height: 84rpx;
	position: absolute;
	left: 50%;
	top: 50%;
	margin:-42rpx 0 0 -42rpx;
	z-index: 2;
	background-image: url('data:image/png;base64,');
	background-size: 100%;
	background-position: center center;
	background-repeat: no-repeat;
}
.video2{
	height: 420rpx;
}
.video2 image{
	width: 100%;
	height: 100%;
}
.content{
	width: auto;
	position: absolute;
	left: 0;
	top: 420rpx;
	right: 0;
	bottom:0;
	z-index: 1;
}
.content.top{
	top: 0;
}
.content .title{
	font-size: 36rpx;
	font-weight: bold;
	color: #1A1A1A;
	padding: 40rpx 40rpx 0 40rpx;
}
.content .tags{
	overflow:hidden;
	margin:20rpx 0 26rpx 40rpx;
}
.content text{
	display: block;
	font-size: 28rpx;
	line-height: 28rpx;
	padding: 6rpx 10rpx;
	color: #fff;
	border-radius: 6rpx;
	background-image: linear-gradient(57deg, #FFBE67 16%, #FF794D 59%, #FF6583 100%);
	float:left;
	margin-right: 20rpx;
}
.content .main{
	color: #474747;
	margin-top: 40rpx;
	overflow:hidden;
	font-size: 28rpx;
	line-height: 2;
	padding:0 40rpx;
}
.praise{
	position: fixed;
	right: -20rpx;
	bottom: -20rpx;
	width: 200rpx;
	height: 100rpx;
	padding-top: 100rpx;
	border-radius: 50%;
	font-size: 24rpx;
	color: #A5A4A4;
	background-repeat: no-repeat;
	background-size: 100%;
	background-position: center center;
	background-image: url('data:image/png;base64,');
	text-align: center;
	-webkit-transition: all 0.2s linear;
	-moz-transition: all 0.2s linear;
	-ms-transition: all 0.2s linear;
	-o-transition: all 0.2s linear;
	transition: all 0.2s linear;
	z-index: 10000;
}
.praise button{
	position: absolute;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	opacity: 0;
}
.praise.on{
	color: #FF5D31;
	background-image: url('data:image/png;base64,');
}


.list{
	margin-top: 40rpx;
}
.list .item .word{
	padding: 0 40rpx;
	line-height: 2;
	margin: 24rpx 0;
}
.list image{
	margin:20rpx 0;
	width: 100%;
}
.list .item:last-child{
	padding-bottom: 60rpx;
}

.material{
	overflow:hidden;
	color: #474747;
	padding: 0 40rpx;
	margin-top: 60rpx;
	margin-bottom: 60rpx;
}
.material .title2{
	font-size: 30rpx;
	font-weight: bold;
	margin-bottom: 14rpx;
	color: #1A1A1A;
	height: 30rpx;
	line-height: 30rpx;
	position: relative;
}
.material .title2::before{
	content:'';
	width: 8rpx;
	height: 30rpx;
	position: absolute;
	left: -40rpx;
	top: 0;
	background-image: -webkit-linear-gradient(45deg, #FF6583 0%, #FF794D 46%, #FFBE67 100%);
	background-image: -moz-linear-gradient(45deg, #FF6583 0%, #FF794D 46%, #FFBE67 100%);
	background-image: -ms-linear-gradient(45deg, #FF6583 0%, #FF794D 46%, #FFBE67 100%);
	background-image: -o-linear-gradient(45deg, #FF6583 0%, #FF794D 46%, #FFBE67 100%);
	background-image: linear-gradient(45deg, #FF6583 0%, #FF794D 46%, #FFBE67 100%);
}
.material .item{
	font-size: 28rpx;
	padding:8rpx 0;
	overflow:hidden;
}
.material .item view{
	width: 50%;
	text-align: left;
}








