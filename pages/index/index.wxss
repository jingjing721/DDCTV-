.wraper{
	padding-left: 60rpx;
	box-sizing: border-box;
	width: 100%;
	height: 100%;
}
.list-title{
	font-size: 56rpx;
	font-weight: bold;
	color: #1A1A1A;
	padding:4rpx 0 4rpx 80rpx;
	position: relative;
	margin: 30rpx 0;
}
.list-title::before{
	content:'';
	width: 60rpx;
	height: 60rpx;
	position: absolute;
	left: 0;
	top: 15rpx;
	background-image: url('data:image/png;base64,');
	background-position: center center;
	background-repeat: no-repeat;
	background-size: 100%;
}
.end{
	font-size: 28rpx;
	color: #E1E1E1;
	text-align: left;
	padding-bottom: 44rpx;
	margin-top: -26rpx;
	position: relative;
}
.end::after{
	content:'';
	width: 72%;
	height: 1px;
	border-bottom:1px solid #EEE;
	position: absolute;
	right:0;
	top:20rpx;
}
.item{}
.item .video{
	height: 390rpx;
	position: relative;
	background-image: url('data:image/jpg;base64,');
	background-color: #eee;
	background-position: center center;
	background-repeat: no-repeat;
	background-size: 120rpx 120rpx;
}
.item .video .time{
	height: 24rpx;
	line-height: 24rpx;
	font-size: 24rpx;
	color: #fff;
	text-align: right;
	position: absolute;
	right: 0;
	bottom: 20rpx;
	padding:4rpx 20rpx 2rpx 0;
	z-index: 2;
	font-family: arial;
}
.item .video .icon{
	width: 84rpx;
	height: 84rpx;
	position: absolute;
	left: 50%;
	top: 50%;
	margin:-42rpx 0 0 -42rpx;
	z-index: 2;
	background-image: url('data:image/png;base64,');
	background-size: 100%;
	background-position: center center;
	background-repeat: no-repeat;
}
.item .video .num{
	width: 46rpx;
	height: 46rpx;
	background-color: #1A1A1A;
	position: absolute;
	top: -10rpx;
	right: 35rpx;
	z-index: 2;
}
.item .video .num::before{
	content:'';
	width: 0;
	height: 0;
	border-left: 23rpx solid #1A1A1A;
	border-top: 10rpx solid transparent;
	border-bottom: 10rpx solid transparent;
	position: absolute;
	left: 0;
	bottom: -10rpx;
}
.item .video .num::after{
	content:'';
	width: 0;
	height: 0;
	border-right: 23rpx solid #1A1A1A;
	border-top: 10rpx solid transparent;
	border-bottom: 10rpx solid transparent;
	position: absolute;
	right: 0;
	bottom: -10rpx;
}
.item .video .num .inner{
	width: 100%;
	height: 100%;
}
.item .video .num .inner::after{
	content:'';
	width: 100%;
	height: 100%;
	display: block;
	background-position: center 5rpx;
	background-size: 100%;
	background-repeat: no-repeat;
}
.list .item:nth-child(1) .num .inner::after{
	background-image: url('data:image/png;base64,');
}
.list .item:nth-child(2) .num .inner::after{
	background-image: url('data:image/png;base64,');
}
.list .item:nth-child(3) .num .inner::after{
	background-image: url('data:image/png;base64,');
}
.list .item:nth-child(4) .num .inner::after{
	background-image: url('data:image/png;base64,');
}
.list .item:nth-child(5) .num .inner::after{
	background-image: url('data:image/png;base64,');
}
.list .item:nth-child(6) .num .inner::after{
	background-image: url('data:image/png;base64,');
}
.list .item:nth-child(7) .num .inner::after{
	background-image: url('data:image/png;base64,');
}
.list .item:nth-child(8) .num .inner::after{
	background-image: url('data:image/png;base64,');
}
.list .item:nth-child(9) .num .inner::after{
	background-image: url('data:image/png;base64,');
}
.list .item:nth-child(10) .num .inner::after{
	background-image: url('data:image/png;base64,');
}
.item .video video{
	width: 100%;
	height: 390rpx;
}
.item .video image{
	width: 100%;
	height: 390rpx;
	position: absolute;
	left: 0;
	top: 0;
	z-index: 1;
}
.item .content{}
.item .content .title{
	font-size: 44rpx;
	padding: 30rpx 0 10rpx 0;
	color: #121212;
	font-weight: bold;
}
.item .content .desc{
	color: #A5A4A4;
	font-size: 28rpx;
	line-height: 1.4;
}
.item .content .control{
	height: 90rpx;
	font-size: 24rpx;
	font-family: arial;
	color: #a5a4a4;
	overflow:hidden;
	padding-bottom: 50rpx;
}
.item .content .control .heart{
	height: 32rpx;
	font-size: 30rpx;
	color: #A5A4A4;
	line-height: 32rpx;
	position: relative;
	padding:30rpx 30rpx 30rpx 38rpx;
}
.item .content .control .heart::before{
	content:'';
	width: 32rpx;
	height: 32rpx;
	position: absolute;
	left: 0;
	top: 30rpx;
	background-position: center center;
	background-size: 100%;
	background-repeat: no-repeat;
	background-image: url('data:image/png;base64,');
}
.item .content .control .heart.on::before{
	background-image: url('data:image/png;base64,');
}
.item .content .control .go{
	width: 120rpx;
	height: 120rpx;
	float: right;
	background-position: center center;
	background-size: 100%;
	background-repeat: no-repeat;
	background-image: url('data:image/png;base64,');
}

.classify{
	width: 104rpx;
	height: 42rpx;
	padding-top: 62rpx;
	background-color: rgba(0,0,0,.8);
	border-radius: 50%;
	position: fixed;
	right: 50rpx;
	bottom: 50rpx;
	z-index: 99999;
	text-align: center;
	font-size: 20rpx;
	color: #fff;
}
.classify::before{
	content:'';
	width: 40rpx;
	height: 40rpx;
	position: absolute;
	left: 50%;
	top: 20rpx;
	margin-left: -20rpx;
	background-position: center center;
	background-size: 100%;
	background-repeat: no-repeat;
	background-image: url('data:image/png;base64,');
}
















