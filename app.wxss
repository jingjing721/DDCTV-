page{
	width: 100%;
	height: 100%;
	position: relative;
	background: #fff;
	font-size: 26rpx;
	color: #232323;
}
button{
    margin:0;
    background:none;
    border:0;
    background-color: none;
    border:0 solid transparent;
}
.one-line {
    white-space: nowrap;
    -webkit-text-overflow: ellipsis;
    -moz-text-overflow: ellipsis;
    -o-text-overflow: ellipsis;
    -ms-text-overflow: ellipsis;
    text-overflow: ellipsis;
    overflow: hidden;
}

.two-line {
    display: -webkit-box; 
    overflow: hidden;
    -webkit-text-overflow: ellipsis;
    -moz-text-overflow: ellipsis;
    -o-text-overflow: ellipsis;
    -ms-text-overflow: ellipsis;
    text-overflow: ellipsis;
    -webkit-box-orient: vertical;
    -moz-box-orient: vertical;
    -o-box-orient: vertical;
    -ms-box-orient: vertical;
    box-orient: vertical;
    -webkit-line-clamp: 2;
    -moz-line-clamp: 2;
    -o-line-clamp: 2;
    -ms-line-clamp: 2;
    line-clamp: 2;
}
.left{float:left;}
.right{float:right;}
.gray { 
    -webkit-filter: grayscale(100%);
    -moz-filter: grayscale(100%);
    -ms-filter: grayscale(100%);
    -o-filter: grayscale(100%);
    filter: grayscale(100%);
    filter: gray;
}
.shareMark{
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 999;
    background-color: rgba(0,0,0,0.5);
}
.shareMark .shareBox{
    width: 670rpx;
    height: 250rpx;
    background-color: #fff;
    border-radius: 8rpx;
    text-align: center;
    position:fixed;
    left: 40rpx;
    right: 40rpx;
    bottom: 40rpx;
}
.shareMark .shareBox .btns{
    float: left;
    margin-left: 154rpx;
    margin-top: 44rpx;
    padding:0;
    font-size: 24rpx;
    color: #1A1A1A;
}
.shareMark .shareBox .btns view{
    line-height: 1.5;
}
.shareMark .shareBox .btns::after{
    border:0;
}
.shareMark .shareBox .img{
    width: 104rpx;
    height: 104rpx;
    margin-bottom: 14rpx;
}
.shareMark .shareBox image{
    width: 100%;
    height: 100%;
    background-size: 100%;
    background-position: center center;
    background-repeat: no-repeat;
}
.shareMark .shareBox .btn1 image{
    background-image: url('data:image/png;base64,');
}
.shareMark .shareBox .btn2 image{
    background-image: url('data:image/png;base64,');
}

button:hover{
    background:none;
}
.savePic{
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    z-index: 99999;
    background:rgba(0,0,0,0.5);
}
.savePic .pic{
    width: 540rpx;
    height: 960rpx;
    position: fixed;
    left: 50%;
    margin-left: -270rpx;
    top: 60rpx;
    border-radius: 8rpx;
    z-index: 1;
    background-position: center center;
    background-size: 100%;
    background-repeat: no-repeat;
    background-image: url('data:image/png;base64,');
}
.savePic .pic .close{
    width: 24rpx;
    height: 24rpx;
    position: fixed;
    right: 110rpx;
    top: 60rpx;
    z-index: 2;
    padding: 24rpx;
    background-size: 33%;
    background-position: center center;
    background-repeat: no-repeat;
    background-image: url('data:image/png;base64,');
}
.savePic .pic image{
    z-index: 1;
    width: 100%;
    height: 100%;
}
.savePic .saveBtn{
    position: fixed;
    left: 50%;
    bottom: 30rpx;
    width: 410rpx;
    margin-left: -210rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    color: #fff;
    font-size: 32rpx;
    background-image: linear-gradient(57deg, #FF6583 16%, #FF794D 59%, #FFBE67 100%);
    border-radius: 100px;
}





